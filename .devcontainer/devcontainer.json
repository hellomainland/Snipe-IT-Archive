{
    "name": "Snipe-it",
    "service": "cli",
    "remoteUser": "user",
    "shutdownAction": "stopCompose",
    "workspaceFolder": "/workspaces/snipe-it",
    "dockerComposeFile": "docker-compose.yaml",
    "forwardPorts": [8000, 3306],

    "extensions": [
        "bmewburn.vscode-intelephense-client",
        "eamodio.gitlens",
        "EditorConfig.EditorConfig",
        "mikestead.dotenv",
        "onecentlin.laravel-blade"
    ],
    "postStartCommand": "bash /workspaces/snipe-it/.devcontainer/bash/startup.sh"
}
